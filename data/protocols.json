{
  "protocols": [
    {
      "id": "stemi",
      "name": "STEMI Protocol",
      "category": "cardiac",
      "keywords": ["chest pain", "crushing", "pressure", "heart attack", "mi", "myocardial infarction", "st elevation", "diaphoretic", "sweating", "radiating arm", "jaw pain"],
      "indications": ["Chest pain with ST-elevation on ECG", "Suspected acute MI", "Crushing/pressure chest pain with diaphoresis"],
      "contraindications": [],
      "steps": [
        {"order": 1, "action": "Obtain 12-lead ECG", "critical": true},
        {"order": 2, "action": "Establish IV access", "critical": true},
        {"order": 3, "action": "Administer oxygen if SpO2 < 94%", "critical": false},
        {"order": 4, "action": "Administer Aspirin 324mg PO (chew)", "critical": true, "drugRef": "aspirin"},
        {"order": 5, "action": "Administer Nitroglycerin 0.4mg SL if SBP > 90", "critical": false, "drugRef": "nitroglycerin"},
        {"order": 6, "action": "Consider Morphine 2-4mg IV for pain", "critical": false, "drugRef": "morphine"},
        {"order": 7, "action": "Activate cath lab / transport to PCI center", "critical": true},
        {"order": 8, "action": "Continuous cardiac monitoring", "critical": true}
      ],
      "medications": [
        {"drug": "aspirin", "dose": "324mg", "route": "PO", "note": "Chewable preferred"},
        {"drug": "nitroglycerin", "dose": "0.4mg", "route": "SL", "note": "May repeat x3 q5min if SBP > 90"},
        {"drug": "morphine", "dose": "2-4mg", "route": "IV", "note": "Titrate to pain relief"}
      ],
      "redFlags": ["Hypotension (SBP < 90)", "Right ventricular infarction", "Recent PDE5 inhibitor use (no NTG)"],
      "transportPriority": "immediate"
    },
    {
      "id": "cardiac-arrest",
      "name": "Cardiac Arrest / ACLS",
      "category": "cardiac",
      "keywords": ["unresponsive", "not breathing", "no pulse", "pulseless", "cpr", "code", "arrest", "vfib", "vtach", "asystole", "pea"],
      "indications": ["Unresponsive patient", "Absent pulse", "Agonal/absent respirations"],
      "contraindications": ["Valid DNR/POLST"],
      "steps": [
        {"order": 1, "action": "Confirm unresponsive, no pulse, no breathing", "critical": true},
        {"order": 2, "action": "Begin high-quality CPR: 100-120/min, 2 inches depth", "critical": true},
        {"order": 3, "action": "Attach defibrillator/AED", "critical": true},
        {"order": 4, "action": "If shockable (VF/VT): Defibrillate 200J biphasic", "critical": true},
        {"order": 5, "action": "Resume CPR immediately for 2 minutes", "critical": true},
        {"order": 6, "action": "Establish IV/IO access", "critical": true},
        {"order": 7, "action": "Epinephrine 1mg IV/IO q3-5min", "critical": true, "drugRef": "epinephrine"},
        {"order": 8, "action": "If refractory VF/VT: Amiodarone 300mg IV", "critical": false, "drugRef": "amiodarone"},
        {"order": 9, "action": "Advanced airway when appropriate", "critical": false},
        {"order": 10, "action": "Identify reversible causes (H's and T's)", "critical": true}
      ],
      "medications": [
        {"drug": "epinephrine", "dose": "1mg", "route": "IV/IO", "note": "Repeat every 3-5 minutes"},
        {"drug": "amiodarone", "dose": "300mg", "route": "IV", "note": "First dose; may repeat 150mg"},
        {"drug": "lidocaine", "dose": "1-1.5mg/kg", "route": "IV", "note": "Alternative to amiodarone"}
      ],
      "redFlags": ["Prolonged downtime", "Asystole > 20 min", "No ROSC after 30 min"],
      "transportPriority": "immediate"
    },
    {
