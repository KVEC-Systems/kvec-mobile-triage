    {
      "id": "hypoglycemia",
      "name": "Hypoglycemia Protocol",
      "category": "metabolic",
      "keywords": ["low blood sugar", "diabetic", "insulin", "confused", "sweating", "shaky", "glucose", "hypoglycemia", "altered mental status"],
      "indications": ["Blood glucose < 60mg/dL", "Symptomatic hypoglycemia", "Altered LOC in known diabetic"],
      "contraindications": [],
      "steps": [
        {"order": 1, "action": "Check blood glucose", "critical": true},
        {"order": 2, "action": "If conscious and can swallow: Oral glucose 15-20g", "critical": false},
        {"order": 3, "action": "If unconscious/unable to swallow: D10 or D50 IV", "critical": true, "drugRef": "dextrose"},
        {"order": 4, "action": "If no IV: Glucagon 1mg IM", "critical": false, "drugRef": "glucagon"},
        {"order": 5, "action": "Recheck glucose in 15 minutes", "critical": true},
        {"order": 6, "action": "Repeat treatment if glucose remains < 60", "critical": false},
        {"order": 7, "action": "Determine cause (missed meal, insulin error, etc.)", "critical": false}
      ],
      "medications": [
        {"drug": "dextrose", "dose": "D10 100mL or D50 25mL", "route": "IV", "note": "D10 preferred for neonates/pediatrics"},
        {"drug": "glucagon", "dose": "1mg", "route": "IM/IN", "note": "If no IV access; slower onset"}
      ],
      "redFlags": ["Persistently low glucose despite treatment", "Sulfonylurea overdose", "Altered LOC not improving"],
      "transportPriority": "urgent"
    },
    {
      "id": "opioid-overdose",
      "name": "Opioid Overdose Protocol",
      "category": "toxicology",
      "keywords": ["overdose", "heroin", "fentanyl", "opioid", "not breathing", "pinpoint pupils", "unresponsive", "narcan", "naloxone"],
      "indications": ["Respiratory depression with suspected opioid use", "Pinpoint pupils", "Unresponsive with track marks or paraphernalia"],
      "contraindications": [],
      "steps": [
        {"order": 1, "action": "Ensure scene safety (possible fentanyl exposure)", "critical": true},
        {"order": 2, "action": "Open airway, provide BVM ventilation", "critical": true},
        {"order": 3, "action": "Naloxone 2mg IN or 0.4mg IV/IM", "critical": true, "drugRef": "naloxone"},
        {"order": 4, "action": "Continue ventilatory support", "critical": true},
        {"order": 5, "action": "Repeat Naloxone q2-3min if no response (max 10mg)", "critical": false},
        {"order": 6, "action": "Maintain airway - prepare for vomiting", "critical": true},
        {"order": 7, "action": "Transport - observe for renarcotization", "critical": true}
      ],
      "medications": [
        {"drug": "naloxone", "dose": "2mg IN or 0.4-2mg IV/IM", "route": "IN/IV/IM", "note": "May repeat q2-3min; titrate to respiratory effort"}
      ],
      "redFlags": ["Fentanyl may require higher doses", "Risk of acute withdrawal", "Renarcotization if long-acting opioid"],
      "transportPriority": "immediate"
    },
    {
